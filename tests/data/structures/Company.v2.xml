<?xml version="1.0" encoding="UTF-8"?>
<sql:datasource xmlns:sql="http://xsd.nore.fr/sql/2.0" xmlns:xi="http://www.w3.org/2001/XInclude">
	<sql:namespace name="ns_unittests">
		<sql:table name="Employees" id="nsut.tables.Employees">
			<sql:comment>Company employees</sql:comment>
			<sql:column name="id">
				<sql:datatype nullable="no">
					<sql:numeric />
				</sql:datatype>
			</sql:column>
			<sql:column name="fullName">
				<sql:datatype>
					<sql:string />
				</sql:datatype>
			</sql:column>
			<sql:column name="age">
				<sql:datatype>
					<sql:numeric min="18" max="65" />
				</sql:datatype>
			</sql:column>
			<sql:column name="gender">
				<sql:datatype>
					<sql:string length="1">
						<sql:enumeration>
							<sql:value>M</sql:value>
							<sql:value>F</sql:value>
							<sql:value>N</sql:value>
						</sql:enumeration>
					</sql:string>
				</sql:datatype>
			</sql:column>
			<sql:column name="salary">
				<sql:datatype>
					<sql:numeric length="7" scale="2" />
				</sql:datatype>
			</sql:column>
			<sql:primarykey>
				<sql:column name="id" />
			</sql:primarykey>
		</sql:table>

		<sql:index name="index_employees_name">
			<sql:tableref name="Employees" />
			<sql:column name="fullName" />
		</sql:index>

		<sql:table name="Hierarchy">
			<sql:column name="managerId">
				<sql:datatype nullable="no" />
			</sql:column>
			<sql:column name="manageeId">
				<sql:datatype nullable="no">
					<sql:numeric />
				</sql:datatype>
			</sql:column>

			<sql:primarykey>
				<sql:column name="managerId" />
				<sql:column name="manageeId" />
			</sql:primarykey>

			<sql:foreignkey name="hierarchy_managerId_foreignkey">
				<sql:column name="managerId" />
				<sql:reference>
					<sql:tableref id="nsut.tables.Employees" />
					<sql:column name="id" />
				</sql:reference>
				<sql:actions>
					<sql:onupdate action="cascade" />
					<sql:ondelete action="cascade" />
				</sql:actions>
			</sql:foreignkey>
			<sql:foreignkey>
				<sql:column name="manageeId" />
				<sql:reference>
					<sql:tableref name="Employees" />
					<sql:column name="id" />
				</sql:reference>
				<sql:actions>
					<sql:onupdate action="cascade" />
					<sql:ondelete action="cascade" />
				</sql:actions>
			</sql:foreignkey>
		</sql:table>

		<sql:table name="Tasks">
			<sql:column name="id">
				<sql:datatype>
					<sql:numeric autoincrement="yes" />
				</sql:datatype>
			</sql:column>
			<sql:column name="name">
				<sql:datatype>
					<sql:string length="64" />
				</sql:datatype>
			</sql:column>
			<sql:column name="creationDateTime">
				<sql:comment>Creation timestamp</sql:comment>
				<sql:datatype>
					<sql:timestamp />
				</sql:datatype>
				<sql:default>
					<sql:timestamp />
				</sql:default>
			</sql:column>
			<sql:column name="category">
				<sql:datatype>
					<sql:numeric />
				</sql:datatype>
			</sql:column>
			<sql:column name="creator">
				<sql:default>
					<sql:null />
				</sql:default>
			</sql:column>


			<sql:column name="assignedTo">
				<sql:default>
					<sql:null />
				</sql:default>
			</sql:column>

			<sql:primarykey name="pk_tid">
				<sql:column name="id" />
			</sql:primarykey>

			<sql:foreignkey name="fk_creator">
				<sql:column name="creator" />
				<sql:reference>
					<sql:tableref id="nsut.tables.Employees" />
					<sql:column name="id" />
				</sql:reference>
				<sql:actions>
					<sql:onupdate action="cascade" />
					<sql:ondelete action="cascade" />
				</sql:actions>
			</sql:foreignkey>

			<sql:foreignkey>
				<sql:column name="assignedTo" />
				<sql:reference>
					<sql:tableref name="Employees" />
					<sql:column name="id" />
				</sql:reference>
				<sql:actions>

					<sql:ondelete action="cascade" />
					<sql:onupdate action="cascade" />
				</sql:actions>
			</sql:foreignkey>
		</sql:table>

		<sql:table name="Products">
			<sql:column name="id">
				<sql:datatype nullable="no">
					<sql:binary />
				</sql:datatype>
				<sql:default>
					<sql:hexBinary>600df00d</sql:hexBinary>
				</sql:default>
			</sql:column>
			<sql:column name="name" />
			<sql:column name="productManagerId">
				<sql:datatype>
					<sql:numeric />
				</sql:datatype>
			</sql:column>

			<sql:primarykey>
				<sql:column name="id" />
			</sql:primarykey>
			<sql:foreignkey>
				<sql:column name="productManagerId" />
				<sql:reference>
					<sql:tableref id="nsut.tables.Employees" />
					<sql:column name="id" />
				</sql:reference>
				<sql:actions>
					<sql:ondelete action="null" />
					<sql:onupdate action="null" />
				</sql:actions>
			</sql:foreignkey>
		</sql:table>

	</sql:namespace>
</sql:datasource>
