{
	"name": "noresources/ns-php-sql",
	"description": "SQL-related class library for PHP",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Renaud Guillard",
			"email": "dev@nore.fr",
			"role": "lead"
		}
	],
	"support": {
		"issues": "https://github.com/noresources/ns-php-sql/issues"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
